{
  "name": "recipe-box",
  "version": "0.0.0",
  "main": "out/electron/main",
  "scripts": {
    "ng": "ng",
    "start": "ng build && electron-forge start",
    "start:dev": "npm run build:electron && electron-forge start",
    "build": "ng build",
    "build:electron": "tsc -p tsconfig.electron.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --watch=false --configuration=ci",
    "lint:html": "htmlhint src/**/*.html",
    "lint:html:fix": "prettier --write src/**/*.html",
    "lint:angular": "ng lint",
    "lint:angular:fix": "ng lint --fix",
    "package": "npm run build && npm run build:electron && electron-forge package",
    "make": "npm run build && npm run build:electron && electron-forge make",
    "lint": "ng lint"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.1.0",
  "dependencies": {
    "@angular/animations": "^21.0.1",
    "@angular/cdk": "^21.0.0",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/material": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "electron-squirrel-startup": "^1.0.1",
    "rxjs": "~7.8.0",
    "sqlite3": "^5.1.7",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.0",
    "@angular-eslint/builder": "21.0.1",
    "@angular-eslint/eslint-plugin": "21.0.1",
    "@angular-eslint/eslint-plugin-template": "21.0.1",
    "@angular-eslint/schematics": "21.0.1",
    "@angular-eslint/template-parser": "21.0.1",
    "@angular/build": "^21.0.0",
    "@angular/cli": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@types/jasmine": "^5.1.13",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "angular-eslint": "^21.0.1",
    "electron": "^39.2.3",
    "electron-reloader": "^1.2.3",
    "eslint": "8.57.1",
    "htmlhint": "^1.8.0",
    "jasmine-core": "^5.12.1",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "typescript": "~5.9.2",
    "zone.js": "^0.15.1"
  }
}